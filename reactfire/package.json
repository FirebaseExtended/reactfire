{
  "name": "reactfire-dev",
  "version": "2.0.0",
  "description": "Firebase library for React",
  "main": "index.js",
  "types": "index.d.ts",
  "private": true,
  "scripts": {
    "build-dev": "tsc --watch",
    "test-dev": "jest --verbose --watch",
    "emulators": "firebase emulators:start --only firestore,database",
    "test": "yarn build && firebase emulators:exec --only firestore,database \"jest --rootDir pub --no-cache --verbose --detectOpenHandles --forceExit\"",
    "copy-package-json": "cp package.pub.json pub/reactfire/package.json",
    "watch": "yarn build && tsc --watch",
    "build": "rm -rf pub && tsc && yarn copy-package-json && cp ../README.md pub/reactfire/README.md && cp ../LICENSE pub/reactfire/LICENSE && rollup -c && node after.build.js && cp ./.npmignore pub/reactfire/ && npm pack ./pub/reactfire"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Firebase/reactfire.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Firebase/reactfire/issues"
  },
  "homepage": "https://firebaseopensource.com/projects/firebase/reactfire/",
  "dependencies": {
    "rxfire": "^3.6.6",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@firebase/app": "^0.4.8",
    "@firebase/app-types": "^0.4.0",
    "@firebase/testing": "^0.16.10",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@testing-library/jest-dom": "^4.1.1",
    "@testing-library/react": "^9.3.0",
    "@testing-library/react-hooks": "^3.1.0",
    "@types/jest": "^24.9.0",
    "babel-jest": "^24.9.0",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^7.1.0",
    "globalthis": "^1.0.1",
    "jest": "~24.9.0",
    "react-test-renderer": "^16.9.0",
    "rollup": "^1.26.3",
    "typescript": "^3.4.5"
  },
  "jest": {
    "setupFiles": [
      "../jest.setup.js"
    ]
  }
}