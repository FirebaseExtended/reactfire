{
  "name": "reactfire",
  "version": "2.0.0",
  "description": "Firebase library for React",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build-dev": "tsc",
    "test-dev": "jest --verbose --watch",
    "test": "firebase emulators:exec --only firestore --project=rxfire-525a3 \"jest --no-cache\"",
    "watch": "tsc index.ts --lib DOM,ES2018 --watch --sourceMap --declaration --jsx react",
    "build": "rm -rf pub && tsc && cp package.pub.json pub/reactfire/package.json && cp ../README.md pub/reactfire/README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Firebase/reactfire.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Firebase/reactfire/issues"
  },
  "homepage": "https://firebaseopensource.com/projects/firebase/reactfire/",
  "dependencies": {
    "@firebase/testing": "^0.11.4",
    "@testing-library/react": "^8.0.4",
    "@testing-library/react-hooks": "^1.0.4",
    "firebase-functions-test": "^0.1.6",
    "rxfire": "^3.6.3",
    "rxjs": "^6.4.0"
  },
  "peerDependencies": {
    "firebase": "^6.0.0",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@firebase/app": "^0.4.8",
    "@firebase/app-types": "^0.4.0",
    "@types/jest": "^24.0.11",
    "babel-jest": "^24.7.1",
    "firebase-tools": "^7.0.2",
    "jest": "~24.7.1",
    "jest-dom": "^3.1.3",
    "react-test-renderer": "^16.8.6",
    "typescript": "^3.4.5"
  }
}
